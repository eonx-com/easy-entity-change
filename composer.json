{
    "name": "eonx-com/easy-entity-change",
    "description": "Provides an easy way to hook up logic in your entities lifecycle",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "psr/event-dispatcher": "^1.0",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4",
        "eonx-com/test-utils": "^0.0.1",
        "eonx-com/standards": "^0.3.0",
        "phpmd/phpmd": "^2.8",
        "phpstan/phpstan": "^0.12.42",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpstan/phpstan-phpunit": "^0.12.3",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.5",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyEntityChange\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyEntityChange\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
