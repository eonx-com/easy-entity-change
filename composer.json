{
    "name": "eonx-com/easy-entity-change",
    "description": "Provides an easy way to hook up logic in your entities lifecycle",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "eonx-com/externals": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyEntityChange\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyEntityChange\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
